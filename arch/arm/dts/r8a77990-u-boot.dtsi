#include "r8a779x-u-boot.dtsi"

/ {
	aliases {
		/delete-property/ mmc0;
		/delete-property/ mmc1;
		/delete-property/ mmc2;
	};

	soc {
		rpc: spi@ee200000 {
			compatible = "renesas,rpc-r8a77990", "renesas,rpc";
			reg = <0 0xee200000 0 0x100>, <0 0x08000000 0 0>;
			clocks = <&cpg CPG_MOD 917>;
			bank-width = <2>;
			status = "disabled";
		};
	};
};
